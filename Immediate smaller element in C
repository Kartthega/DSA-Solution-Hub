//To find immediate smaller element in C
#include<stdio.h>
void immediatesmallele(int *arr,int n){
  for(int i=0;i<n-1;i++){
    if(arr[i+1]<arr[i]){
      printf("%d",arr[i+1]);
  }
    else{
      printf("-1");
  }
}
printf("-1");
return;
}

int main(){

int n;
printf("Enter array size:");
scanf("%d",&n);

int arr[n];
for(int i=0;i<n;i++){
 printf("Enter array elements:");
 scanf("%d",&arr[i]);
}

immediatesmallele(arr,n);

return 0;
}
