//Largest element in C

#include<stdio.h>

void largerelement(int *arr,int n){
int max=-9999;
 for(int i=0;i<n;i++){
   if(arr[i]>max){
     max=arr[i];
 }
}

printf("The Larger element in the array:%d",max);

}

int main(){
int n;
printf("Enter the size of array:");
scanf("%d",&n);

int arr[n];
printf("enter elements of array:\n");
for(int i=0;i<n;i++){
  scanf("%d",&arr[i]);
}

largerelement(arr,n);

return 0;

}
